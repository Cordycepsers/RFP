  "reference_number_patterns": {
    "un_agencies": [
      "RFP/\\d{4}/\\d{3,4}",
      "ITB/\\d{4}/\\d{3,4}",
      "RFQ/\\d{4}/\\d{3,4}",
      "EOI/\\d{4}/\\d{3,4}"
    ],
    "world_bank": [
      "P\\d{6}",
      "TF\\d{6}",
      "IBRD\\d{5}"
    ],
    "ngos": [
      "[A-Z]{2,4}/\\d{4}/[A-Z]+/\\d{3}",
      "[A-Z]{3,5}-\\d{4}-\\d{3}",
      "REF-\\d{4}-\\d{3,4}"
    ],
    "generic": [
      "\\d{4}-\\d{3,4}",
      "[A-Z]{2,4}\\d{4,6}",
      "\\d{6,8}"
    ]
  },
  "scraper_configs": {
    "ungm": {
      "max_pages": 10,
      "results_per_page": 15,
      "request_delay": 2.0,
      "reference_patterns": [
        "RFP/\\d{4}/[A-Z]+/[A-Z]{3}/\\d+",
        "UNDP-[A-Z]{3}-\\d{5,6}",
        "UNW-[A-Z]{3}-\\d{4}-\\d{5}-\\d",
        "RFQ/[A-Z]+/\\d{4}/\\d+",
        "RFP\\d{7}",
        "\\d{4}/[A-Z]+/[A-Z]+/\\d+",
        "UNDP-HQ-\\d{5}",
        "LRPS-\\d{4}-\\d{7}",
        "LRFQ-\\d{7}",
        "EOIOSESGY\\d{5}"
      ]
    },
    "globalfund": {
      "max_tenders": 50,
      "request_delay": 3.0,
      "reference_patterns": [
        "TGF-\\d{2}-\\d{2,3}(?:,\\d+)?"
      ],
      "oracle_timeout": 60
    },
    "worldbank": {
      "max_pages": 5,
      "max_opportunities": 100,
      "results_per_page": 20,
      "request_delay": 2.0,
      "reference_patterns": [
        "P\\d{6}",
        "WB-\\d{4}-\\d{3}",
        "IBRD-\\d{5}",
        "IDA-\\d{5}"
      ]
    }
  },
